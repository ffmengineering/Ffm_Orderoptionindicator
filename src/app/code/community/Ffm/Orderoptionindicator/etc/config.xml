<config>
    <modules>
        <Ffm_Orderoptionindicator>
            <version>0.1.1</version>
        </Ffm_Orderoptionindicator>
    </modules>
    <global>
        <blocks>
            <ffm_orderoptionindicator>
                <class>Ffm_Orderoptionindicator_Block</class>
            </ffm_orderoptionindicator>
        </blocks>
        <helpers>
            <ffm_orderoptionindicator>
                <class>Ffm_Orderoptionindicator_Helper</class>
            </ffm_orderoptionindicator>
        </helpers>
    </global>
    <adminhtml>
        <layout>
            <updates>
                <ffm_orderoptionindicator>
                    <file>ffm_orderoptionindicator.xml</file>
                </ffm_orderoptionindicator>
            </updates>
        </layout>
    </adminhtml>
</config>